				<attribute name="padding-top">5</attribute>
				<attribute name="padding-right">5</attribute>
				<attribute name="padding-bottom">5</attribute>
				<attribute name="padding-left">5</attribute>

				<attribute name="margin-left">5</attribute>
			</style>
		</class>
		<class name="WebNavigationField" ref="ActionField">
			<style>
				<attribute name="text-decoration">underline</attribute>
				<attribute name="text-underline-style">dotted</attribute>
			</style>
		</class>
		<class name="EmailActionField" ref="WebNavigationField" />
		<class name="ActionLabelField" ref="ActionField" />
		<class name="ScreenNavigationField" ref="ActionField" />

		<!--
			
			labels
			
		-->
		<class name="LabelField" ref="h4" />
		<class name="AdCountdownLabel" ref="h3" />
		<class name="Media-header-title" ref="h5">
			<style>
				<attribute name="color">rgb(255, 127, 0)</attribute>

				<attribute name="margin-left">5</attribute>
				<attribute name="margin-right">5</attribute>
			</style>
		</class>

		<!-- 
		
			h* definitions
			
		-->
		<class name="h1">
			<style>
				<attribute name="font-family">BBAlpha Sans</attribute>
				<attribute name="font-size">8</attribute>
				<attribute name="font-weight">bold</attribute>

				<attribute name="color">rgb(200, 200, 200)</attribute>
				<attribute name="focus-color">rgb(200, 200, 200)</attribute>

				<attribute name="margin-top">3</attribute>
				<attribute name="margin-bottom">3</attribute>
				<attribute name="margin-left">10</attribute>
			</style>
		</class>
		<class name="h2" ref="h1">
			<style>
				<attribute name="font-size">7</attribute>

				<attribute name="margin-top">2</attribute>
				<attribute name="margin-bottom">2</attribute>
			</style>
		</class>
		<class name="h3" ref="h2">
			<style>
				<attribute name="font-size">6</attribute>

				<attribute name="margin-top">1</attribute>
				<attribute name="margin-bottom">1</attribute>
			</style>
		</class>
		<class name="h4" ref="h3"><!-- normal font equivalent -->
			<style>
				<attribute name="font-weight">normal</attribute>

				<attribute name="margin-top">0</attribute>
				<attribute name="margin-bottom">0</attribute>
			</style>
		</class>
		<class name="h5" ref="h4">
			<style>
				<attribute name="font-size">5</attribute>
				<attribute name="font-weight">bold</attribute>
			</style>
		</class>
		<class name="h6" ref="h5">
			<style>
				<attribute name="font-weight">normal</attribute>
			</style>
		</class>
		<class name="h7" ref="h6">
			<style>
				<attribute name="font-size">4</attribute>
			</style>
		</class>
	</classes>
</theme>
